@using System.Activities.Statements
@model IEnumerable<Webservice.Models.Measurement_Table>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<div class="table-responsive">
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Compound_Table.StofNavn)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Station_Table.Navn)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Units_Table.Navn)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UTM_Table.GeometriId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.datoMaerke)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Resultat)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Compound_Table.StofNavn)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Station_Table.Navn)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Units_Table.Navn)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.UTM_Table.GeometriId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.datoMaerke)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Resultat)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new {id = item.Id}) |
                    @Html.ActionLink("Details", "Details", new {id = item.Id}) |
                    @Html.ActionLink("Delete", "Delete", new {id = item.Id})
                </td>
            </tr>
        }
    </table>
</div>